<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.bDao.BangDao">
    <select id="getBangById" resultType="Bang" parameterType="String">
        select * from bang where id = #{id}
    </select>
    <select id="getAllBang" resultType="Bang">
        select * from bang
    </select>
</mapper>